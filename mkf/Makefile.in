BUILD_TOPDIR = ..

all .DEFAULT:
	# "cd lib" must be enclosed by () because it may not return
	# to the $(top_builddir) at the next line.
	(cd lib ; $(MAKE) $@)
	cd libtbl ; $(MAKE) $@

distclean : clean
	rm -f Makefile config.log config.cache config.status libtool config.mk lib/Makefile libtbl/Makefile
