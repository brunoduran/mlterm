/* Assume 'i' is UCS4(32bit) */
#define  HI(i)  ( ((i)>>8) & 0xffffff )
#define  LO(i)  ( (i) & 0xff )

inline u_int8_t
CONV_UCS4_TO_CP1252(
	u_int32_t  ch
	)
{
	u_int32_t  hi_ch ;
	u_int32_t  hi_beg ;
	u_int8_t *  table ;
	ucs4_to_cp1252_range_t *  range ;

	if( ch < ucs4_to_cp1252_beg || ucs4_to_cp1252_end < ch)
	{
		return  0 ;
	}
	
	hi_ch = HI(ch) ;
	hi_beg = HI(ucs4_to_cp1252_beg) ;
	
	if( ! ( table = ucs4_to_cp1252_tables[ hi_ch - hi_beg].table))
	{
		return  0 ;
	}

	range = ucs4_to_cp1252_tables[ hi_ch - hi_beg].range ;
	
	if( ch < range->beg || range->end < ch)
	{
		return  0 ;
	}

	return  table[ LO(ch) - LO(range->beg)] ;
}

