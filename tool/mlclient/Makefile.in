prefix = @prefix@
exec_prefix = @exec_prefix@
bindir = @bindir@

CC = @CC@
CFLAGS = @KIK_CFLAGS@ @CFLAGS@
INSTALL = @INSTALL@

BINDIR = $(DESTDIR)$(bindir)

OBJ = main.o

PROG = mlclient

all: $(PROG)

$(PROG): $(OBJ)
	$(CC) -o $(PROG) $(OBJ)

.SUFFIXES: .c.o

.c.o:
	$(CC) $(CFLAGS) -c $<

clean:
	rm -f $(OBJ) $(PROG) $(PROG).core

install:
	$(INSTALL) -m 755 $(PROG) $(BINDIR)
